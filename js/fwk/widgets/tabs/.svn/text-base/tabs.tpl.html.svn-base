<nav id="tab-nav{{id}}" class="tab-nav ui-helper-gradient ui-help-tabs-border">
  <ul class="popup-option-list">
    {{#tabs}}
    <li id="{{tabId}}">
      <a href="" data-tab-order="{{tabOrder}}">
        {{#if icon}}
        <span  class="sprite-{{icon}}-ico ui-icon left" data-tab-order="{{tabOrder}}"></span>
        {{/if}}
        {{tabName}}
      </a>
    </li>
    {{/tabs}}
  </ul>
</nav>
{{#tabs}}
<div id="{{tabPanel}}" class="tab-panel ui-help-tabs-border tab-border-top"></div>
{{/tabs}}
