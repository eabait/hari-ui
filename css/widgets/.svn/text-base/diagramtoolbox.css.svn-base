.diagram-toolbox {
  position: absolute;
  z-index: 100;
  background-color: #f8f8f8;
  border: 1px solid silver;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
}

.diagram-toolbox ul {
  list-style-type: none;
  padding: 10px 12px 7px 12px;
}

.diagram-toolbox ul li{
  display: inline-block;
  cursor: pointer;
  margin: 0 2px 0 2px;
}
