/*  Resets
------  */

html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6,
p, blockquote, pre, a, abbr, address, cite, code, del, dfn, em,
img, ins, kbd, q, samp, small, strong, sub, sup, var, b, i, hr,
dl, dt, dd, ol, ul, li, fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figure, figcaption, hgroup,
menu, footer, header, nav, section, summary, time, mark, audio, video {
  margin: 0;
  padding: 0;
  border: 0;
}

article, aside, canvas, figure, figure img, figcaption, hgroup,
footer, header, nav, section, audio, video {
  display: block;
}

a img {border: 0;}

/*
 * Global styles:
 */

 html, body {
  height: 100%;
  width: 1280px;
  margin: 0 auto;
}

header {
  width: 100%;
  margin: 0 auto;
  background: #354f88; /* for non-css3 browsers */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#354f88', endColorstr='#317aae',GradientType=0 ); /* IE6-9 */
  background-image: linear-gradient(right , rgb(53,79,136) 20%, rgb(49,122,174) 60%, rgb(53,79,136) 100%);
  background-image: -o-linear-gradient(right , rgb(53,79,136) 20%, rgb(49,122,174) 60%, rgb(53,79,136) 100%);
  background-image: -moz-linear-gradient(right , rgb(53,79,136) 20%, rgb(49,122,174) 60%, rgb(53,79,136) 100%);
  background-image: -webkit-linear-gradient(right , rgb(53,79,136) 20%, rgb(49,122,174) 60%, rgb(53,79,136) 100%);
  background-image: -ms-linear-gradient(right , rgb(53,79,136) 20%, rgb(49,122,174) 60%, rgb(53,79,136) 100%);
  background-image: -webkit-gradient(linear, right top, left top, color-stop(0.2, rgb(53,79,136)), color-stop(0.6, rgb(49,122,174)), color-stop(1, rgb(53,79,136)));
}

body {
  font-family: Arial, Helvetica, sans-serif;
  background-image: url(../images/body-background.png);
}

footer {
  bottom: 2%;
  margin: 0 auto 0 25px;
  position: fixed;
  width: 1230px;
}

.main-container{
  width: 1230px;
  margin: 0 auto;
  height: 440px;
}

